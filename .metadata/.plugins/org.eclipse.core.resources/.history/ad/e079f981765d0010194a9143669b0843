package com.tss.factory.TransportApp.model;

import java.util.Random;

public class TwoWheeler implements LicenceGenerator{

	@Override
	public String LicenceGenerator() {
		Random random = new Random();

        int fourDigit = 1000 + random.nextInt(9000); // 9000 = 9999 - 1000 + 1
        System.out.println("The number for two wheeler" + "4-digit number: " + fourDigit);

        // Generate a 6-digit number (100000 to 999999)
        int sixDigit = 100000 + random.nextInt(900000); // 900000 = 999999 - 100000 + 1
        System.out.println("6-digit number: " + sixDigit);
		
		return null;
	}

}
