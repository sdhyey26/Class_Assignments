package com.tss.CarApp.test;

import com.tss.CarApp.model.CarFactory;
import com.tss.CarApp.model.CarTypes;
import com.tss.CarApp.model.Icars;

public class CarTest {
	public static void main(String[] args) {
		CarFactory factory = new CarFactory();
		
		CarTypes car = CarTypes.Mahindra;
		
		Icars cars = factory.makeCars(car);
		
		cars.start();
		cars.stop();
	}
}
