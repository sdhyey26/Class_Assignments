package com.tss.structural.Adapter.model;

public class HatAdapter implements Iitems{

	Hat hat = new Hat();
	
	@Override
	public String getName() {
		// TODO Auto-generated method stub
		return  hat.getLongName() + hat.getShortName();
	}

	@Override
	public int getPrice() {
		// TODO Auto-generated method stub
		return hat.getPrice();
	}

}
