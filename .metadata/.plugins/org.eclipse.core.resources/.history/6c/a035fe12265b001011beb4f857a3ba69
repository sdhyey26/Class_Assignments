package com.tss.test;

import com.tss.model.FestivalType;
import com.tss.model.FixedDeposit;

public class Main {
	public static void main(String[] args) {
        FixedDeposit fd1 = new FixedDeposit(1, "Dhyey", 4000, 6, FestivalType.NEWYEAR);
        FixedDeposit fd2 = new FixedDeposit(2, "Vivek", 5000, 5, FestivalType.DIWALI);

        System.out.println("FD1 Interest: " + fd1.calculateSimpleInterest());
        System.out.println("FD2 Interest: " + fd2.calculateSimpleInterest());
    }
}