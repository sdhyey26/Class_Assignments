package com.tss.withoutViolation.model;

public class InvoiceModel {
	
	public InvoiceModel() {
		super();
	}
	private int id;
    private String description;
    private double cost;
    private double discountPercent;
	private final double tax = 10.0;
        
    public static double getTax() {
		return tax;
	}
	
	public static int getId() {
		return id;
	}
	public static void setId(int id) {
		InvoiceModel.id = id;
	}
	public static String getDescription() {
		return description;
	}
	public static void setDescription(String description) {
		InvoiceModel.description = description;
	}
	public static double getCost() {
		return cost;
	}
	public static void setCost(double cost) {
		InvoiceModel.cost = cost;
	}
	public static double getDiscountPercent() {
		return discountPercent;
	}
	public static void setDiscountPercent(double discountPercent) {
		InvoiceModel.discountPercent = discountPercent;
	}

}
